{{#if isVisible}}
	<li class="mumble-channel">
		<i class="{{iconClass}} mumble-channel-icon"></i> <a href {{action "expand"}} class="mumble-channel-name">{{data.name}}</a>
		<section {{bind-attr class="expanded::hidden :mumble-channel-content"}}>
			<ul>
			{{#each user in usersList}}
				{{mumble-user data=user}}
			{{/each}}
			</ul>
			<ul>
			{{#each c in children}}
				{{mumble-channel data=c}}
			{{/each}}
			</ul>
		</section>
	</li>
{{/if}}
